  "scripts": {
    "build": "NODE_ENV=production webpack --config static.webpack.config.prod.js",
    "build-sit": "NODE_ENV=production webpack --mode production",
    "copy-secrets": "cp secret.config.json.example secret.config.json",
    "migrate": "node-pg-migrate",
    "start": "NODE_ENV=development webpack-dev-server --open --config static.webpack.config.dev.js --host 0.0.0.0 --public jet.intrepid-geophysics.com",
    "start-sec": "NODE_ENV=development webpack-dev-server --open --config static.webpack.config.dev.js --host 0.0.0.0 --public jet.intrepid-geophysics.com --server-type https --server-options-key /etc/letsencrypt/live/jet.intrepid-geophysics.com/privkey.pem --server-options-ca /etc/letsencrypt/live/jet.intrepid-geophysics.com/chain.pem --server-options-cert /etc/letsencrypt/live/jet.intrepid-geophysics.com/cert.pem",
    "start-hot": "NODE_ENV=development webpack-dev-server --open --hot --config static.webpack.config.dev.js",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 jest",
    "silent-test": "NODE_OPTIONS=--max_old_space_size=4096 jest --silent",
    "test:watch": "NODE_OPTIONS=--max_old_space_size=4096 jest --watchAll",
    "start:ci": "webpack-dev-server --config static.webpack.config.dev.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run -c video=false",
    "cypress:ci": "NODE_ENV=test_cypress start-server-and-test start:ci http-get://localhost:8080 cypress:run",
    "cypress:prepare-ci": "bin/cypress-prepare-ci"
